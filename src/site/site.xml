<project>
	<bannerLeft>
		<!-- Reflow Maven Skin, but with "Reflow" highlighted -->
		<name><![CDATA[
                <span class="color-highlight">Leo</span>
                ]]></name>
	</bannerLeft>
	<version position="bottom"/>
	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.0.0</version>
	</skin>
	<custom>
		<reflowSkin>
			<theme>bootswatch-cerulean</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<imgLightbox>true</imgLightbox>
			<brand>
				<name>Leo</name>
			</brand>
			<slogan>Making UIMA Easy.</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<topNav>Home|Learn|Download</topNav>
			<bottomNav maxSpan="9">
				<column>Download</column>
				<column>Learn</column>
				<column>reports</column>
			</bottomNav>
			<bottomDescription><![CDATA[ <strong>Leo</strong><br/>
                 Making UIMA Easy.   ]]></bottomDescription>
			<breadcrumbs>false</breadcrumbs>
			<markPageHeader>true</markPageHeader>
			<skinAttribution>false</skinAttribution>
			<pages>
				<index>
				</index>
				<userguide>
					<highlightJs>true</highlightJs>
					<toc>top</toc>
				</userguide>
				<components>
					<highlightJs>true</highlightJs>
					<toc>top</toc>
				</components>
				<faq>
					<highlightJs>false</highlightJs>
					<toc>top</toc>
				</faq>
				
				<license_apache>
					<highlightJs>false</highlightJs>
				</license_apache>
				<!-- Disable ToC for some Maven reports -->
				<project-info>
					<toc>true</toc>
				</project-info>
				<dependencies>
					<tocTopMax>4</tocTopMax>
				</dependencies>
			</pages>
		</reflowSkin>
	</custom>
	<body>
		<links>
			<item name="Home" href="index.html"/>
		</links>
		<menu name="Learn" inherit="top">
			<item name="User Guide" href="userguide.html"/>
			<item name="Components and Tools" href="components.html"/>
			<item name="FAQ" href="faq.html"/>
			<item name="Javadoc" href="apidocs/index.html"/>
		</menu>
		<menu name="Download" inherit="top">
			<item name="License" href="license_apache.html"/>
			<item name="Download" href="download.html"/>
		</menu>
		<menu name="Contribute" inherit="bottom">
			<item name="Contribute" href="contribute.html"/>
			<item name="Issues" href="http://github.com/andriusvelykis/reflow-maven-skin/issues/"/>
			<item name="Fork on GitHub" href="http://github.com/andriusvelykis/reflow-maven-skin/"/>
			<item name="Donate" href="https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;hosted_button_id=QWKNRFZH52828"/>
		</menu>
		<!-- <menu name="Maven documentation" ref="reports" inherit="bottom"/> -->
	</body>
</project>
